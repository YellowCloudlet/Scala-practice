import scala.math.sqrt
import scala.math.pow

val gold = (1 + sqrt(5)) / 2
val sqrt5 = sqrt(5)

def fibanacci(n: Int) = {
 BigDecimal(pow(gold,n) / sqrt5).toBigInt
}

def fibBefore(n: Int) = {
  for(i <- 0 to n - 1){
    print(fibanacci(i) + " ")
  }
  println(" ")
}

val test = List(7,6,5,4,3,2,1,0)

test.map(i => fibBefore(i))
